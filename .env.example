# API Provider Configuration
# Choose either 'openai' or 'openrouter'
VITE_API_PROVIDER=openrouter

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-4o-mini
VITE_OPENAI_MAX_TOKENS=2000
VITE_OPENAI_TEMPERATURE=0.7

# OpenRouter Configuration
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here
VITE_OPENROUTER_MODEL=google/gemini-2.0-flash-exp:free
# Other OpenRouter models:
# - google/gemini-2.0-pro-exp-02-05:free
# - google/gemini-2.0-flash-thinking-exp:free
# - deepseek/deepseek-r1-distill-llama-70b:free
# - openrouter/quasar-alpha

# General LLM Settings (applies to both providers)
VITE_LLM_MAX_TOKENS=2000
VITE_LLM_TEMPERATURE=0.7
VITE_LLM_MAX_RETRIES=5
VITE_LLM_RETRY_DELAY=5000

# Optional OpenAI Configuration
VITE_OPENAI_BASE_URL=
VITE_OPENAI_ORGANIZATION=
VITE_OPENAI_PROJECT=